{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "backend",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-24",
  "compatibility_flags": ["nodejs_compat"],
  "workflows": [
    {
      "name": "generate-quiz",
      "binding": "GENERATE_QUIZ",
      "class_name": "GenerateQuizWorkflow"
    }
  ],
  "triggers": {
    "crons": ["*/1 * * * *"]
  },
  "vars": {
    "OPENAI_BASE_URL": "https://openrouter.ai/api/v1",
    "OPENAI_MODEL": "x-ai/grok-4.1-fast"
  }
}
